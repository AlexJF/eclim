<!DOCTYPE beans PUBLIC
  "-//SPRING//DTD BEAN//EN"
  "http://www.springframework.org/dtd/spring-beans.dtd">

<!--
 Author: Eric Van Dewoestine
-->
<beans>

  <import resource="command/spring-factory.xml"/>

  <bean class="org.eclim.preference.PreferenceFactory"
      factory-method="addOptions">
    <constructor-arg value="org.python.pydev.pythonNature"/>
    <constructor-arg>
      <value>
        PyDev org.python.pydev.INTERPRETER_PATH
        PyDev org.python.pydev.PYTHON_PROJECT_VERSION python 2\.[3-5]
        PyDev org.python.pydev.PROJECT_SOURCE_PATH
        PyDev org.python.pydev.PROJECT_EXTERNAL_SOURCE_PATH
      </value>
    </constructor-arg>
  </bean>

</beans>
